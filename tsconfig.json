{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "./env.d.ts",
    "./src/**/*",
    "./src/**/*.vue"
  ],
  "compilerOptions": {
    "importsNotUsedAsValues": "error",
    "baseUrl": ".",
    "typeRoots": ["./src/types"],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}